{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@img/*": [
        "src/assets/images/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
